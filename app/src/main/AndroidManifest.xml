<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.dim.ui">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.USE_FINGERPRINT"/>

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".FingerActivity">
        </activity>
        <activity android:name=".EditActivity">
        </activity>
        <activity android:name=".FunctionActivity">
        </activity>
        <activity
            android:name=".FillActivity"
            android:windowSoftInputMode="stateHidden|stateUnchanged|adjustPan">
        </activity>
        <activity android:name=".ModifyActivity">
        </activity>
        <activity android:name=".AccountActivity">
        </activity>
        <activity android:name=".ConfirmActivity">
        </activity>
        <activity android:name=".ModifyPasswordActivity">
        </activity>
    </application>

</manifest>