<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:paddingRight="10dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="考生来源"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="invisible"/>

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/et_26"
            android:entries="@array/spinner_kaoshen_laiyuan"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="毕业学校"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_27"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/fill_edit"
            android:hint="填写全称"
            android:textSize="@dimen/fill_text_size"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="毕业专业"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_28"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/fill_edit"
            android:hint="填写全称"
            android:textSize="@dimen/fill_text_size"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="取得最后学历的学习形式"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="invisible"/>

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/et_29"
            android:entries="@array/spinner_xuexi_xinshi"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="right"
            android:spinnerMode="dialog"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="最后学历"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="invisible"/>

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/et_30"
            android:entries="@array/spinner_zuihou_xueli"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="right"
            android:spinnerMode="dialog"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="毕业证书编号"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_31"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            style="@style/fill_edit"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="获得最后学历毕业年月"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <TextView
            android:id="@+id/et_32"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/fill_edit"
            android:hint="点击选择日期"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册学号"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_33"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            style="@style/fill_edit"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="最后学位"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <Button
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:visibility="invisible"/>

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/et_34"
            android:entries="@array/spinner_zuihou_xuewei"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:gravity="right"
            android:spinnerMode="dialog"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="学位证书编号"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_35"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:inputType="number"
            style="@style/fill_edit"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="报考单位"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_36"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/fill_edit"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/view_line"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="报考专业"
            android:textColor="@color/text_color"
            android:textSize="@dimen/text_size"/>

        <EditText
            android:id="@+id/et_37"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/fill_edit"/>
    </LinearLayout>

</LinearLayout>